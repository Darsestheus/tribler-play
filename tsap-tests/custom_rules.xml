<?xml version="1.0" encoding="UTF-8"?>
<project name="MainActivityTest" default="help">

<target name="fetch-test-report">
	<echo>Downloading XML test reportâ€¦</echo>
	<mkdir dir="junitreports"/>
	<exec executable="${adb}" failonerror="true">
		<arg line="${adb.device.arg}"/>
		<arg value="pull" />
		<arg value="/data/data/org.tribler.tsap/files/junit-report.xml" />
		<arg value="junitreports/junit-report.xml" />
	</exec>
</target>

</project>
